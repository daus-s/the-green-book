create table
  public.user_bets (
    "userID" bigint generated by default as identity,
    amount bigint not null,
    "betID" bigint null,
    outcome text null,
    "userbetID" bigint generated by default as identity,
    constraint user_bets_pkey primary key ("userbetID"),
    constraint user_bets_betID_fkey foreign key ("betID") references bets ("betID"),
    constraint user_bets_userID_fkey foreign key ("userID") references user_balances ("userID")
  ) tablespace pg_default;

  create table
  public.users (
    "userID" bigint generated by default as identity,
    email text not null,
    constraint users_pkey primary key ("userID"),
    constraint users_email_key unique (email)
  ) tablespace pg_default;

  create table
  public.user_balances (
    "userID" bigint generated by default as identity,
    balance bigint not null default '0'::bigint,
    constraint user_balances_pkey primary key ("userID")
  ) tablespace pg_default;

  create table
  public.commissioners (
    "commissionerID" bigint generated by default as identity,
    "userID" bigint not null,
    email text null,
    constraint commissioners_pkey primary key ("commissionerID"),
    constraint commissioners_email_fkey foreign key (email) references users (email),
    constraint commissioners_userID_fkey foreign key ("userID") references users ("userID")
  ) tablespace pg_default;

  create table
  public.bets (
    "betID" bigint generated by default as identity,
    title character varying not null,
    description character varying null,
    odds jsonb null,
    creationtime timestamp with time zone not null,
    open boolean null default false,
    "commissionerID" bigint not null,
    constraint bets_pkey primary key ("betID"),
    constraint bets_commissionerID_fkey foreign key ("commissionerID") references commissioners ("commissionerID")
  ) tablespace pg_default;